{% block form_row -%}
    <div class="form_row">
        {{- form_label(form) -}}
        {{- form_widget(form) -}}
		{{- form_errors(form) -}}
    </div>
{%- endblock form_row %}

{% block form_errors -%}
    {% if errors|length > 0 -%}
		{%- for error in errors -%}
			<span class="error">{{ error.message }}</span>
		{%- endfor -%}
    {%- endif %}
{%- endblock form_errors %}